<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="init-data1" author="Arkadii Dolgonos" runOnChange="true">
        <loadUpdateData tableName="contact" file="db/data/init/Contact.csv"
                        primaryKey="id"/>
        <loadUpdateData tableName="ext_Product_Model" file="db/data/init/CarModels.csv"
                        primaryKey="id"/>
        <loadUpdateData tableName="ext_Product_Brand" file="db/data/init/CarBrands.csv"
                        primaryKey="id"/>
        <loadUpdateData tableName="ext_Product" file="db/data/init/Cars.csv"
                        primaryKey="id"/>
        <loadUpdateData tableName="manufacturer" file="db/data/init/Manufacturers.csv"
                        primaryKey="id"/>
        <loadUpdateData tableName="product_Group" file="db/data/init/Product_Group.csv"
                        primaryKey="id"/>
        <loadUpdateData tableName="person" file="db/data/init/Person.csv"
                        primaryKey="id"/>
        <loadUpdateData tableName="provider" file="db/data/init/Provider.csv"
                        primaryKey="id"/>
        <loadUpdateData tableName="person_Contact_Lnk" file="db/data/init/Person_Contact_Lnk.csv"
                        primaryKey="person_id,contact_id"/>
        <loadUpdateData tableName="provider_Person_Lnk" file="db/data/init/Provider_Person_Lnk.csv"
                        primaryKey="provider_id,person_id"/>
        <loadUpdateData tableName="product" file="db/data/init/Product.csv"
                        primaryKey="id"/>
        <loadUpdateData tableName="product_Extproduct_Lnk" file="db/data/init/Product_Extproduct_Lnk.csv"
                        primaryKey="product_id,ext_product_id"/>
        <loadUpdateData tableName="product_Price" file="db/data/init/Product_Price.csv"
                        primaryKey="product_id,date_start"/>
        <loadUpdateData tableName="product_Cost" file="db/data/init/Product_Cost.csv"
                        primaryKey="product_id,date_start"/>
        <loadUpdateData tableName="product_Provider_Lnk" file="db/data/init/Product_Provider_Lnk.csv"
                        primaryKey="product_id,provider_id"/>
    </changeSet>

</databaseChangeLog>